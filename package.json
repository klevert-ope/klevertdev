  },
  "dependencies": {
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.5.5",
    "cssnano": "^6.1.2",
    "dompurify": "^3.1.0",
    "gsap": "./gsap-bonus.tgz",
